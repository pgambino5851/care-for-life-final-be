{
  "name": "careforlife-apollo",
  "description": "Apollo layer for Prismatopia",
  "version": "1.0.0",
  "author": "Lambda Labs",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "server": "nodemon index.js",
    "coverage": "npm test -- --coverage --watchAll=false || true",
    "test": "cross-env NODE_ENV=testing node_modules/.bin/jest",
    "test -CI": "jest --watch=all",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "eslint": "eslint --fix ./src"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/__tests__/**"
    ]
  },
  "dependencies": {
    "@okta/jwt-verifier": "^1.0.0",
    "apollo-server": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.1",
    "graphql-import-node": "0.0.4",
    "graphql-tools": "^4.0.7",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.7.0",
    "prisma-client-lib": "^1.34.10",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "cross-env": "^7.0.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
